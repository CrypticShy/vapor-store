{
  "name": "vapor-store",
  "version": "2.0.0-beta-2",
  "description": "Vapor Store",
  "author": "SushyDev",
  "main": "app.js",
  "license": "GPL-3.0",
  "homepage": "https://github.com/SushyDev/vapor-store",
  "scripts": {
    "build": "electron-builder -w",
    "start": "APP_DEV=true electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SushyDev/vapor-store.git"
  },
  "bugs": {
    "url": "https://github.com/SushyDev/vapor-store"
  },
  "build": {
    "appId": "vapor.store",
    "productName": "Vapor Store",
    "asar": true,
    "publish": [{
      "provider": "github",
      "owner": "SushyDev",
      "repo": "vapor-store",
      "releaseType": "prerelease"
    }],
    "files": [
      "**/*",
      "!**/node_modules/puppeteer/linux-756035${/*}",
      "app/json/*"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/png/icon.png"
    }
  },
  "dependencies": {
    "@material/card": "^8.0.0",
    "@material/feature-targeting": "^8.0.0",
    "@material/linear-progress": "^8.0.0",
    "@material/progress-indicator": "^8.0.0",
    "@material/ripple": "^8.0.0",
    "@material/theme": "^8.0.0",
    "electron-dl": "^3.0.2",
    "extract-zip": "^2.0.1",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1",
    "lozad": "^1.16.0",
    "material-components-web": "^7.0.0",
    "material-design-icons-webfont": "^1.0.0",
    "puppeteer": "3.3.0",
    "request": "^2.88.2",
    "sass": "^1.6.2",
    "steamgriddb": "^1.4.0"
  },
  "devDependencies": {
    "electron": ">=7.2.4",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0"
  }
}