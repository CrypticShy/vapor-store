{
  "name": "vapor-store",
  "version": "1.5.1",
  "description": "Vapor Store",
  "homepage": "",
  "main": "assets/electron/app.js",
  "scripts": {
    "start": "electron .",
    "make": "electron-builder -w"
  },
  "build": {
    "appId": "vapor.store",
    "productName": "Vapor Store",
    "asar": true,
    "files": [
      "**/*",
      "!node_modules/puppeteer/.local-chromium${/*}"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": "nsis",
      "icon": "assets/images/icons/ico/icon.ico"
    }
  },
  "keywords": [
    "Vapor",
    "Store"
  ],
  "author": "Sushy",
  "license": "GNU GPLv3",
  "devDependencies": {
    "electron": "^8.3.0",
    "electron-builder": "^22.6.0"
  },
  "dependencies": {
    "@octokit/rest": "^17.9.2",
    "cheerio": "^1.0.0-rc.3",
    "fs": "0.0.1-security",
    "igdb-api-node": "^4.0.4",
    "jquery": "^3.5.1",
    "open-file-explorer": "^1.0.2",
    "opn": "^6.0.0",
    "progress-extract": "^1.0.2",
    "puppeteer": "^3.1.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "^3.0.2"
  }
}
